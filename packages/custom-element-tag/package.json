{
  "name": "@uzh-bf/custom-element-tag",
  "version": "1.6.1",
  "type": "module",
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.8.3",
    "@types/preact-custom-element": "^4.0.1",
    "npm-run-all": "4.1.5",
    "parcel": "2.8.3",
    "postcss": "8.4.23",
