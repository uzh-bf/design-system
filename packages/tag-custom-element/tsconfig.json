{
  "compilerOptions": {
    "target": "ES6",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsxImportSource": "preact",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "react": ["../../node_modules/preact/compat/"],
      "react-dom": ["../../node_modules/preact/compat/"]
    },
    "moduleResolution": "node"
  }
}
