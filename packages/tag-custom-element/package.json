{
  "name": "@uzh-bf/design-system-custom-element-tag",
  "version": "4.0.9",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@uzh-bf/design-system-react-tag": "workspace:*",
    "htm": "3.1.1",
    "preact": "10.13.2",
    "preact-custom-element": "4.2.1",
    "tailwind-merge": "1.12.0"
  },
  "devDependencies": {
    "@parcel/plugin": "2.12.0",
    "@parcel/transformer-inline-string": "2.15.4",
    "@types/node": "^20.0.0",
    "@types/preact-custom-element": "^4.0.1",
    "@uzh-bf/parcel-config-design-system": "workspace:*",
    "@uzh-bf/parcel-resolver-preact": "workspace:*",
    "npm-run-all": "4.1.5",
    "parcel": "2.12.0",
    "postcss": "8.4.23",
    "rimraf": "5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.8.3"
  },
  "scripts": {
    "build": "run-s build:purge build:parcel",
    "build:parcel": "parcel build",
    "build:purge": "rimraf dist",
    "dev": "parcel index.html --open"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime",
    "react/jsx-dev-runtime": "preact/jsx-runtime"
  },
  "module": "dist/index.js",
  "source": "src/index.ts",
  "targets": {
    "module": {
      "optimize": true,
      "includeNodeModules": {
        "react": false,
        "preact": false,
        "htm": false,
        "preact-custom-element": false,
        "tailwind-merge": false
      }
    }
  },
  "type": "module"
}
