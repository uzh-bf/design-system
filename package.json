{
  "name": "@uzh-bf/design-system",
  "version": "0.1.10",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/uzh-bf/design-system.git"
  },
  "maintainers": [
    "Roland Schlaefli <roland.schlaefli@bf.uzh.ch>",
    "Julius Schlapbach <julius.schlapbach@bf.uzh.ch>",
    "Florina Vogel <florina.vogel@bf.uzh.ch>"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "npm-run-all build:dts build:copy",
    "build:dts": "dts build --entry src/index.ts --entry src/constants.js",
    "build:copy": "node src/copy.js",
    "lint": "dts lint",
    "size": "size-limit",
    "dev": "ladle serve",
    "deploy": "ladle build",
    "format": "prettier --write src/",
    "test": "dts test --passWithNoTests",
    "prerelease": "npm run build",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run",
    "prepare": "husky install"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/index.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ladle/react": "2.4.5",
    "@size-limit/preset-small-lib": "7.0.8",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.8",
    "@tsconfig/create-react-app": "1.0.3",
    "@tsconfig/recommended": "1.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "10.4.13",
    "dts-cli": "1.6.0",
    "eslint": "8.29.0",
    "husky": "8.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "prettier-plugin-organize-imports": "3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-postcss": "4.0.2",
    "size-limit": "7.0.8",
    "standard-version": "9.5.0",
    "tailwindcss": "3.2.4",
    "tailwindcss-radix": "2.6.1",
    "tslib": "2.4.1",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "formik": "2.2.9",
    "tailwind-merge": "1.8.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "1.0.1",
    "@radix-ui/react-collapsible": "1.0.1",
    "@radix-ui/react-dialog": "1.0.2",
    "@radix-ui/react-dropdown-menu": "2.0.1",
    "@radix-ui/react-label": "2.0.0",
    "@radix-ui/react-navigation-menu": "1.1.1",
    "@radix-ui/react-progress": "1.0.1",
    "@radix-ui/react-select": "1.1.2",
    "@radix-ui/react-slider": "1.1.0",
    "@radix-ui/react-switch": "1.0.1",
    "@radix-ui/react-tabs": "1.0.1",
    "@radix-ui/react-tooltip": "1.0.2",
    "react-countdown-circle-timer": "3.1.0"
  }
}
